PYDAW_VERSION=$(cat ../major-version.txt)
BIN=$PYDAW_VERSION"_render"
PROJECT=$HOME/$PYDAW_VERSION/default-project

make clean && make release && \
perf stat -e cache-references,cache-misses,dTLB-loads,dTLB-load-misses,\
iTLB-loads,iTLB-load-misses,L1-dcache-loads,L1-dcache-load-misses,\
L1-icache-loads,L1-icache-load-misses,branch-misses,LLC-loads,LLC-load-misses \
./$BIN $PROJECT "test.wav" 0 0 3 0 44100 512 0 1 --no-file

