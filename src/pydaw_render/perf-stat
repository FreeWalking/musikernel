PYDAW_VERSION=$(cat ../major-version.txt)
BIN=$PYDAW_VERSION"_render"
PROJECT=$HOME/$PYDAW_VERSION/default-project

make clean && make release && \
perf stat -e cache-misses,dTLB-store-misses,dTLB-load-misses,iTLB-load-misses,L1-dcache-load-misses,L1-dcache-store-misses,L1-icache-load-misses ./$BIN $PROJECT "test.wav" 0 0 3 0 44100 512 0 1 --no-file

